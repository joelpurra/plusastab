{
  "name": "plusastab",
  "title": "plusastab",
  "version": "0.2.2",
  "description": "A jQuery plugin to use the numpad plus key (configurable) as a tab key equivalent.",
  "homepage": "https://joelpurra.com/projects/plusastab/",
  "bugs": {
    "url": "https://github.com/joelpurra/plusastab/issues"
  },
  "author": "Joel Purra <code@joelpurra.com> (https://joelpurra.com/)",
  "main": "src/plusastab.joelpurra.js",
  "scripts": {
    "build": "rimraf ./docs/dist && mkdirp ./docs/dist && cp ./src/plusastab.joelpurra.js ./docs/dist/",
    "test": "npm run --silent lint",
    "lint": "npm run --silent eslint --",
    "eslint": "eslint ./src ./docs/test",
    "git-add-dist": "git add ./docs/dist/plusastab.joelpurra.js"
  },
  "pre-commit": [
    "build",
    "test",
    "git-add-dist"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mkdirp": "^0.5.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpurra/plusastab.git"
  },
  "keywords": [
    "tab",
    "tabbing",
    "usability",
    "focus",
    "keyboard",
    "shortcuts",
    "tabbable",
    "form",
